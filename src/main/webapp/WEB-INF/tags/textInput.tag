<%@ attribute name="name" %><%@ attribute name="size" %><%@ attribute name="style" required="false" %><%@ attribute name="value" required="false" %><%@ attribute name="readonly" type="java.lang.Boolean" required="false" %><% if (readonly!=null && readonly.booleanValue()){ %><span class=${style}>${value}</span><% } else { %><input class="${style}" type="text" name="${name}" value="${value}" size="${size}" /><jsp:doBody/><% } %>
